# added with 'aws-vault add dnlopes'
[profile dnlopes]
region = eu-west-1
output = json
credential_process = aws-vault export --format=json dnlopes
mfa_serial = $DOTFILES_YUBIKEY_MFA_ARN
mfa_process = ykman oath accounts code --single $DOTFILES_YUBIKEY_MFA_ARN

